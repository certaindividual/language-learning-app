    <children>
        <ComboBox fx:id="categoryChooser" layoutX="108.0" layoutY="65.0" prefWidth="150.0" promptText="Wybierz kategorię" />
        <Button fx:id="startRevisionButton" layoutX="300.0" layoutY="65.0" mnemonicParsing="false" onAction="#handleStartRevisionAction" text="Rozpocznij Powtorke" />
        <TableView fx:id="revisionTable" layoutX="30.0" layoutY="100.0" prefHeight="250" prefWidth="500"/>
        <Button fx:id="goBackButton" onAction="#handleGoBackAction" layoutX="523.0" layoutY="360.0" mnemonicParsing="false" text="Powrót" />
    </children>
</AnchorPane>
